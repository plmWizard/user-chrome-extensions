{
  "manifest_version": 3,
  "name": "Fusion Manage â€“ Second Tabs",
  "version": "1.1.0",
  "description": "Adds a second tabs menu with a 'Second grid' iframe.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.autodeskplm360.net/plm/workspaces/*/items/*"],
      "js": ["content.js"],
      "css": ["content.css"],
      "run_at": "document_idle"
    },
    {
      "matches": ["https://*.autodeskplm360.net/plm/workspaces/*/items/*"],
      "js": ["action-tabs.js", "action-onedit-runner.js"],
      "css": ["action-tabs.css"],
      "run_at": "document_idle",
      "all_frames": true
    },
    {
      "matches": ["https://*.autodeskplm360.net/plm/workspaces/*/items/*"],
      "js": ["header-color.js"],
      "run_at": "document_idle"
    }
  ],
  "host_permissions": ["https://*.autodeskplm360.net/*"]
}
